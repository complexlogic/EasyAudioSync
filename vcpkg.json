{
  "dependencies": [
    {
      "name": "qtbase",
      "platform": "!linux",
      "default-features": false,
      "features": ["gui", "widgets", "png"]
    },
    {
      "name": "qttools",
      "platform": "!linux",
      "default-features": false,
      "features": ["linguist"]
    },
    {
      "name": "ffmpeg",
      "default-features": false,
      "features": ["avcodec", "avformat", "avfilter", "swresample", "mp3lame", "fdk-aac", "vorbis", "opus"]
    },
    "taglib",
    "spdlog"
  ]
}
  